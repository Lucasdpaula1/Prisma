

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model User {
  id Int @id @default(autoincrement())
  name String
  email String
  @@map("users")
  
}
model Fornecedores {
  id Int @id @default(autoincrement())
   name String
  email String
  @@map("fornecedores")



}
